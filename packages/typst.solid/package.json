  },
  "devDependencies": {
    "@myriaddreamin/typst-ts-renderer": "0.5.0-rc9",
    "@myriaddreamin/typst.ts": "0.5.0-rc9",
    "solid-js": "^1.8.22",
    "vite-plugin-solid": "^2.10.2",
    "vite": "^5.4.8"
  },
  "keywords": [
    "solidjs",
    "typst",
    "typescript",
    "vite"
