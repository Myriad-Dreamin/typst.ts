{
  "name": "@myriaddreamin/typst.solid",
  "version": "0.5.0-rc9",
  "description": "A SolidJS package for Typst integration.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
