{
  "name": "@myriaddreamin/typst.react",
  "version": "0.1.2",
  "description": "Typst.ts for React",
  "main": "src/lib/index.js",
  "types": "src/lib/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "build": "tsc -p ./tsconfig.build.json --emitDeclarationOnly --outDir dist && webpack --config webpack/webpack.config.js --env env=prod",
    "build:lib": "tsc -p ./tsconfig.build.json --outDir ./dist/lib && node ./scripts/prepublish.lib.js",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "link:local": "yarn link @myriaddreamin/typst.ts",
		"prepublish": "npm run build:lib",
		"publish:dry": "npm publish --dry-run --access public ./dist/lib",
		"publish": "npm publish --access public ./dist/lib"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Typst"
  ],
  "author": "Myriad-Dreamin",
  "license": "Apache-2.0",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "@myriaddreamin/typst.ts": "^0.1.2"
  },
  "peerDependencies": {      
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/runtime": "^7.21.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^3.2.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.1",
    "prettier": "^2.8.7",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.2",
    "typescript": "^4.9.5",
    "webpack": "^5.79.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.13.3",
    "webpack-merge": "^5.7.3"
  }
}
