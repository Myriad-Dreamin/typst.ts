{
  "name": "@myriaddreamin/typst.react",
  "version": "0.5.0-rc9",
  "description": "Typst.ts for React",
  "author": "Myriad-Dreamin",
  "license": "Apache-2.0",
  "keywords": [
    "React",
    "TypeScript",
    "Typst"
  ],
  "bugs": {
    "url": "https://github.com/Myriad-Dreamin/typst.ts/issues"
  },
  "homepage": "https://myriad-dreamin.github.io/typst.ts/",
  "repository": "https://github.com/Myriad-Dreamin/typst.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.js"
    }
  },
  "files": [
    "dist/**/*.{mts,mjs,js,ts}"
  ],
  "scripts": {
    "dev": "cd demo && vite dev -c vite.config.mjs",
    "build": "tsc --outDir dist && vite build",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "exit 0",
    "publish:dry": "npm publish --dry-run --access public",
    "publish:lib": "npm publish --access public || exit 0"
  },
  "peerDependencies": {
    "@myriaddreamin/typst-ts-renderer": "^0.5.0-rc9",
    "@myriaddreamin/typst.ts": "^0.5.0-rc9",
    "react": "^17.0.1 || ^18.x",
    "react-dom": "^17.0.1 || ^18.x"
  },
  "devDependencies": {
    "@myriaddreamin/typst-ts-renderer": "*",
    "@myriaddreamin/typst.ts": "*",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}